symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	pont	all	always
! sentence ending	felkiáltójel	all	always
? sentence ending	kérdőjel	all	always
; phrase ending	pontosvessző	most	always
: phrase ending	kettőspont	most	always
tizedes vessző		none	always
szóban lévő '	aposztrof	all	norep
negatív szám	minusz	none	norep

# Whitespace
\0	üres	char
\t	tab
\n	újsor	char
\f	oldaltörés	none
\r	sor vége	char
 	szóköz	char
 	szóköz (töretlen)	char	# no-break space

