#, no-wrap
msgid "[[!inline  pages=\"page(addons/*) and currentlang()\" archive=\"yes\"]]\n"
msgstr "[[!inline  pages=\"page(addons/*) and currentlang()\" archive=\"yes\"]]\n"

#. type: Plain text
msgid "This website is being maintained by the NVDA translation team."
msgstr "Tätä sivustoa ylläpitää NVDA:n käännnöstiimi."

