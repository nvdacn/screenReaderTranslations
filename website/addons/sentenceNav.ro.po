#. type: Plain text
#, no-wrap
msgid "[[!meta title=\"SentenceNav\"]]\n"
msgstr "[[!meta title=\"SentenceNav\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tony Malykh"
msgstr "Autor: Tony Malykh"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "[Descarcă versiunea stabilă][1]"

#. type: Plain text
msgid ""
"SentenceNav is an NVDA add-on that allows you to read text by sentences, as "
"opposed to by paragraphs or words."
msgstr ""
"Acest supliment vă permite să navigați de la propoziție la propoziție într-"
"un text"

#. type: Title ##
#, no-wrap
msgid "Keystrokes"
msgstr "Comenzi"

#. type: Bullet: '* '
msgid "Alt+Down: Go to next sentence."
msgstr "Alt+săgeată jos: următoarea propoziție"

#. type: Bullet: '* '
msgid "Alt+Up: Go to previous sentence"
msgstr "Alt+săgeată sus: propoziția anterioară"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Alt+Down: Go to next sentence."
msgid "Alt+Shift+Down: Go to next text paragraph."
msgstr "Alt+săgeată jos: următoarea propoziție"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Alt+Up: Go to previous sentence"
msgid "Alt+Shift+Up: Go to previous text paragraph."
msgstr "Alt+săgeată sus: propoziția anterioară"

