msgid "Authors: Noelia, Chris, Alberto."
msgstr "लेखक: नोलिया, क्रिस र अल्बर्टो"

#. type: Bullet: '*	'
msgid "download [stable version][1]"
msgstr "डाउनलोड[version 1][1]"

#. type: Bullet: '*	'
