msgid "[[!meta title=\"NoBeepsSpeechMode\"]]\n"
msgstr "[[!meta title=\"No Beeps Speech Mode\"]]\n"

#. type: Bullet: '*	 '
msgid "Author: Alberto Buffolino."
msgstr "Auteur: Alberto Buffolino."

#. type: Bullet: '*	 '
msgid "download [stable version][1]"
msgstr "download [stabiele versie][1]"

#. type: Bullet: '*	 '
msgid "download [development version][2]"
msgstr "download [ontwikkelversie][2]"

#. type: Plain text
msgid "This add-on excludes beeps in the speech mode when pressing NVDA+s.  You can only toggle between off or talk."
msgstr "Deze add-on onderdrukt pieptonen in de spraakmodus als u op NVDA+s drukt. U kan enkel wisselen tussen uit en spreek."

#. type: Title ##
#, no-wrap
#| msgid "Changes for 1.5"
msgid "Changes for 2.0"
msgstr "Veranderingen voor 2.0"

#. type: Bullet: '*	 '
msgid "Add-on help is available from the Add-ons Manager."
msgstr "Hulp voor de add-on is beschikbaar in de Add-ons Beheerder."

#. type: Title ##
#, no-wrap
msgid "Changes for 1.5"
msgstr "Veranderingen voor 1.5"

#. type: Bullet: '*	 '
msgid "Added script category (speech)."
msgstr "Scriptcategorie (speech) toegevoegd."

#. type: Bullet: '*	 '
msgid "Check for an older version of the addon which is not community supported, and prompt user before removing it."
msgstr "Controleert of er een oudere versie van de add-on is die niet community supported is en informeert de gebruiker alvorens deze te verwijderen."

#. type: Title ##
#, no-wrap
msgid "Changes for 1.4"
msgstr "Veranderingen voor 1.4"

#. type: Bullet: '*	 '
msgid "From this version, the messages about speech mode are automatically translated from NVDA in the correct language, no specific translation for addon required (with the possible obvious exception of manifest.ini and script documentation)."
msgstr "Vanaf deze versie worden de reeds vertaalde meldingen in verband met spraakmodus overgenomen uit NVDA."

#. type: Title ##
#, no-wrap
msgid "Changes for 1.3"
msgstr "Veranderingen voor 1.3"

#. type: Bullet: '*	 '
msgid "Fixed bug in translations support: added python sources to make strings translatable."
msgstr "Een probleem opgelost met de ondersteuning voor vertalingen: Python-code toegevoegd om teksten vertaalbaar te maken."

#. type: Title ##
#, no-wrap
msgid "Changes for 1.2"
msgstr "Veranderingen voor 1.2"

