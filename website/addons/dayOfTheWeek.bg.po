msgid "[[!meta title=\"Day of the week\"]]\n"
msgstr "[[!meta title=\"Ден от седмицата (Day of the week)\"]]\n"

#. type: Bullet: '*	 '
msgid "Authors: Abdel, Noelia."
msgstr "Автори: Abdel, Noelia."

#. type: Bullet: '*	 '
msgid "download [stable version][1]"
msgstr "Изтегляне на [стабилна версия][1]"

#. type: Bullet: '*	 '
