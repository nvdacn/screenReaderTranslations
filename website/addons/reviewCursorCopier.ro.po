msgid "[[!meta title=\"Review Cursor Copier\"]]\n"
msgstr "[[!meta title=\"Copiatorul cursorului de examinare\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tuukka Ojala <tuukka.ojala@gmail.com>"
msgstr "Autor: Tuukka Ojala <tuukka.ojala@gmail.com>"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "Descărcați [versiunea în dezvoltare][1]"

#. type: Bullet: '* '
