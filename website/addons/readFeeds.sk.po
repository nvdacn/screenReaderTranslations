msgid "[[!meta title=\"Read Feeds\"]]\n"
msgstr "[[!meta title=\"RSS čítačka\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Noelia Ruiz Martínez, Mesar Hameed"
msgstr "Autori: Noelia Ruiz Martínez, Mesar Hameed"

#. type: Bullet: '* '
msgid "Download [stable version][2]"
msgstr "Stiahnuť [stabilná verzia][2]"

#. type: Bullet: '* '
