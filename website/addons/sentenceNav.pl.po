msgid "[[!meta title=\"SentenceNav\"]]\n"
msgstr "[[!meta title=\"SentenceNav\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tony Malykh"
msgstr "Autor: Tony Malykh"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "Pobierz [Wersja stabilna][1]"

#. type: Plain text
msgid ""
"SentenceNav is an NVDA add-on that allows you to read text by sentences, as "
"opposed to by paragraphs or words."
msgstr ""
"Sentence naw to dodatek, umożliwiający czytanie tekstu zdaniami oraz "
"akapitami."

#. type: Title ##
#, no-wrap
msgid "Keystrokes"
msgstr "Skróty klawiszowe"

#. type: Bullet: '* '
msgid "Alt+Down: Go to next sentence."
msgstr "Alt+Down: Przejdź do następnego zdania."

#. type: Bullet: '* '
msgid "Alt+Up: Go to previous sentence"
msgstr "Alt+Up: Przejdź do poprzedniego zdania"

#. type: Bullet: '* '
msgid "Alt+Shift+Down: Go to next text paragraph."
msgstr "Alt+Shift+Down: Przejdź do następnego akapitu."

#. type: Bullet: '* '
msgid "Alt+Shift+Up: Go to previous text paragraph."
msgstr "Alt+Shift+Up: Przejdź do poprzedniego akapitu."

