msgid "[[!meta title=\"Emoticons\"]]\n"
msgstr ""

#. type: Bullet: '* '
msgid "Authors: Chris Leo, Noelia Ruiz MartÃ­nez, Mesar Hameed"
msgstr ""

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr ""

#. type: Bullet: '* '
