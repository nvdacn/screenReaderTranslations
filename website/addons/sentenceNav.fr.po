msgid "[[!meta title=\"SentenceNav\"]]\n"
msgstr "[[!meta title=\"SentenceNav\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tony Malykh"
msgstr "Auteur : Tony Malykh"

#. type: Bullet: '* '
msgid "Download [stable version][1]"
msgstr "Télécharger [version stable][1]"

#. type: Plain text
msgid ""
"SentenceNav is an NVDA add-on that allows you to read text by sentences, as "
"opposed to by paragraphs or words."
msgstr ""
"SentenceNav est un module complémentaire NVDA qui vous permet de lire du "
"texte par phrases, par opposition à des paragraphes ou des mots."

#. type: Title ##
#, no-wrap
msgid "Keystrokes"
msgstr "Combinaisons des touches à frapper"

#. type: Bullet: '* '
msgid "Alt+Down: Go to next sentence."
msgstr "Alt+Bas : Aller à la phrase suivante."

#. type: Bullet: '* '
msgid "Alt+Up: Go to previous sentence"
msgstr "Alt+Haut : Aller à la phrase précédente"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Alt+Down: Go to next sentence."
msgid "Alt+Shift+Down: Go to next text paragraph."
msgstr "Alt+Bas : Aller à la phrase suivante."

#. type: Bullet: '* '
#, fuzzy
#| msgid "Alt+Up: Go to previous sentence"
msgid "Alt+Shift+Up: Go to previous text paragraph."
msgstr "Alt+Haut : Aller à la phrase précédente"

