msgstr "[[!meta title=\"Audio-Schema\"]]\n"

#. type: Bullet: '*   '
msgid "Authors: Musharraf Omer and others"
msgstr "Autoren: Musharraf Omer und andere"

#. type: Bullet: '*   '
msgid "download [stable version][1]"
