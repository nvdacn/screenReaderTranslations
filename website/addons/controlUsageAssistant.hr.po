msgstr "[[!meta title=\"Pomagač korištenja kontrole\"]]\n"

#. type: Bullet: '* '
msgid "Author: Joseph Lee"
msgstr "Autor: Joseph Lee"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Download: [version 2.1][1]"
msgid "Download: [version 2.4][1]"
