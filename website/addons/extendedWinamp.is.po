#. type: Plain text
#, no-wrap
msgid "[[!meta title=\"extendedWinamp\"]]\n"
msgstr "[[!meta title=\"extendedWinamp\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Hrvoje Katic and NVDA-addon-team"
msgstr "Höfundar: Hrvoje Katic og NVDA-viðbótar teymið"

#. type: Bullet: '* '
msgid "Download: [version 1.1][1]"
msgstr "Niðurhal: [version 1.1][1]"

#. type: Plain text
msgid "This extended appModule extends the original Winamp appModule found in NVDA with some extra functionality."
msgstr "Þetta framlengda appModule eykur stuðninginn við Winamp í NVDA með nokkrum viðbótar eiginleikum."

#. type: Bullet: '* '
msgid "s togle shuffle on/off"
msgstr "s skiptir milli shuffle virkt/óvirkt"

#. type: Bullet: '* '
msgid "r togle repeat on/off"
msgstr "r skiptir milli repeat virkt/óvirkt"

#. type: Bullet: '* '
msgid "F5 mute playback"
msgstr "F5 hljóð af í playback"

#. type: Bullet: '* '
msgid "F6 set playback volume to 25%"
msgstr "F6 setur hljóðstyrk í 25% í playback"

#. type: Bullet: '* '
msgid "F7 set playback volume to 50%"
msgstr "F7 setur hljóðstyrk í 50% í playback"

#. type: Bullet: '* '
msgid "F8 set playback volume to 100%"
msgstr "F8 setur hljóðstyrk í 100% í playback"

#. type: Bullet: '* '
msgid "Shift+LeftArrow pan Left"
msgstr "Shift+örVinstri hreyfa Vinstri"

#. type: Bullet: '* '
msgid "Shift+RightArrow pan Right"
msgstr "Shift+örHægri hreyfa Hægri"

#. type: Bullet: '* '
msgid "Shift+UpArrow pan Center"
msgstr "Shift+örUpp hreyfa Miðju"

#. type: Bullet: '* '
msgid "Control+Shift+t speaks total Track Length"
msgstr "Control+Shift+t segir lengd skráar"

#. type: Bullet: '* '
msgid "Control+Shift+e speaks track Elapsed Time"
msgstr "Control+Shift+e segir hversu mikill tími er liðinn frá upphafi spilunnar skráar"

#. type: Bullet: '* '
msgid "Control+Shift+r speaks track Remaining Time"
msgstr "Control+Shift+r segir Remaining Time"

#. type: Bullet: '* '
msgid "Shift+r Review the end of track \"last 6 seconds by default\""
msgstr "Shift+r Review enda skráar \"last 6 seconds by default\""

#. type: Bullet: '* '
msgid "Control+r Set the review time \"in seconds\" for use with Review End of Track command"
msgstr "Control+r Set the review time \"in seconds\" til notkunar með Review End of Track skipun"

#. type: Bullet: '* '
msgid "Shift+j Set alternate jump time \"in seconds\""
msgstr "Shift+j Skilgreina alternate jump time \"in seconds\""

#. type: Bullet: '* '
msgid "Control+RightArrow alternate Jump Forward \"6 seconds by default\""
msgstr "Control+örHægri Jump Forward \"6 seconds by default\""

#. type: Bullet: '* '
msgid "Control+LeftArrow alternate Jump Backward \"6 seconds by default\""
msgstr "Control+örVinstri Jump Backward \"6 seconds by default\""

#. type: Title ##
#, no-wrap
msgid "Changes for 1.1"
msgstr "Breytingar fyrir 1.1"

#. type: Bullet: '* '
msgid "New languages: Aragonese, Arabic, Dutch, German, Finnish, French, Galician, Hungarian, Italian, Japanese, Korean, Nepali, Portuguese (Brazil), Russian, Slovak, Slovenian, Spanish, Tamil, Turkish."
msgstr "Ný tungumál, Aragóníska, Arabíska, Hollenska, Þýska, Finnska, Franska, Galíska, Ungverska, Ítalska, Japanska, Kóreska, Nepalska, Brasilísk Portúgalska, Rússneska, Slóvanska, Spænska, Tamíl, Tyrkneska."

#. type: Title ##
#, no-wrap
msgid "Changes for 1.0"
msgstr "Breytingar fyrir 1.0"

#. type: Bullet: '* '
msgid "Initial Release"
msgstr "Frumútgáfa"

