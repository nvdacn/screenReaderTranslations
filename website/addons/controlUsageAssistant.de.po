msgstr "[[!meta title=\"Hilfe zur Benutzung von Steuerelementen\"]]\n"

#. type: Bullet: '* '
msgid "Author: Joseph Lee"
msgstr "Autor: Joseph Lee"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Download: [version 2.1][1]"
