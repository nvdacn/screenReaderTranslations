msgid "[[!meta title=\"IndentNav\"]]\n"
msgstr "[[!meta title=\"Навигация по отстъпи (IndentNav)\"]]\n"

#. type: Bullet: '* '
msgid "Author: Tony Malykh"
msgstr "Автор: Tony Malykh"

#. type: Bullet: '* '
#, fuzzy
#| msgid "Download [development version][2]"
msgid "Download [stable version][1]"
