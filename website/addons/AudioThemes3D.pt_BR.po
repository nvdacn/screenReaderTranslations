#. type: Plain text
#, no-wrap
msgid "[[!meta title=\"Audio Themes\"]]\n"
msgstr "[[!meta title=\"Temas Sonoros\"]]\n"

#. type: Bullet: '*   '
msgid "Authors: Musharraf Omer and others"
msgstr "Autores: Musharraf Omer e outros"

#. type: Bullet: '*   '
msgid "download [stable version][1]"
msgstr "baixe a [versão estável][1]"

