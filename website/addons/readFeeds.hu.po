msgid "[[!meta title=\"Read Feeds\"]]\n"
msgstr "[[!meta title=\"Hírcsatorna-olvasó\"]]\n"

#. type: Bullet: '* '
msgid "Authors: Noelia Ruiz Martínez, Mesar Hameed"
msgstr "Készítők: Noelia Ruiz Martínez, Mesar Hameed"

#. type: Bullet: '* '
msgid "Download [stable version][2]"
msgstr "Letöltés [stabil verzió][2]"

#. type: Bullet: '* '
