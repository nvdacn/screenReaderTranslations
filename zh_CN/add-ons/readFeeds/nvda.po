#, fuzzy
msgid "Copy Error"
msgstr "备份错误"

#. Translators: Message presented when feeds have been restored.
#: addon\globalPlugins\readFeeds\__init__.py:78
msgid "Feeds restored"
msgstr "恢复Feeds"

#. Translators: The title of a dialog.
#: addon\globalPlugins\readFeeds\__init__.py:102
msgid "Feeds: {defaultFeed} ({configProfile})"
msgstr "({Feed默认)}({Feed配置文件}) "

