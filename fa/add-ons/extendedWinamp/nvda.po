#: addon/appModules/winamp.py:60
#, python-format
msgid "%d hour, "
msgstr "%d ساعت"

#: addon/appModules/winamp.py:62
#, python-format
msgid "%d hours, "
msgstr "%d ساعت"

#: addon/appModules/winamp.py:64
#, python-format
msgid "%d minute, "
msgstr "%d دقیقه"

#: addon/appModules/winamp.py:66
#, python-format
msgid "%d minutes, "
msgstr "%d دقیقه"

#: addon/appModules/winamp.py:68
#, python-format
msgid "%s second"
msgstr "%s ثانیه"

#: addon/appModules/winamp.py:70
#, python-format
msgid "%s seconds"
msgstr "%s ثانیه"

#: addon/appModules/winamp.py:127 addon/appModules/winamp.py:137
msgid "on"
msgstr "روشن"

#: addon/appModules/winamp.py:129 addon/appModules/winamp.py:139
msgid "off"
msgstr "خاموش"

#: addon/appModules/winamp.py:144
msgid "mute"
msgstr "بی‌صدا"

#: addon/appModules/winamp.py:145
msgid "Mute playback"
msgstr "بی‌صدا کردنِ پخش"

#: addon/appModules/winamp.py:150
msgid "Set playback volume to 25%"
msgstr "تنظیمِ حجمِ صدای پخش روی ۲۵ درصد"

#: addon/appModules/winamp.py:155
msgid "Set playback volume to 50%"
msgstr "تنظیمِ حجمِ صدای پخش روی ۵۰ درصد"

#: addon/appModules/winamp.py:160
msgid "Set playback volume to 100%"
msgstr "تنظیمِ حجمِ صدای پخش روی ۱۰۰ درصد"

#: addon/appModules/winamp.py:164
msgid "Pan left"
msgstr "پخشِ صدا از چپ"

#: addon/appModules/winamp.py:168
msgid "Pan right"
msgstr "پخشِ صدا از راست"

#: addon/appModules/winamp.py:172
msgid "center"
msgstr "وسط"

#: addon/appModules/winamp.py:173
msgid "Pan center"
msgstr "پخشِ صدا از هر دو سو"

#: addon/appModules/winamp.py:178 addon/appModules/winamp.py:185
#: addon/appModules/winamp.py:192
msgid "No time information."
msgstr "اطلاعاتِ زمان موجود نیست"

#: addon/appModules/winamp.py:180
msgid "Speaks total track length"
msgstr "مدت زمانِ کلیِ تِرَکِ فعلی را اعلام میکند."

#: addon/appModules/winamp.py:187
msgid "Speaks track elapsed time"
msgstr "مدت زمانِ گذشته از تِرَکِ در حالِ پخش را اعلام میکند."

#: addon/appModules/winamp.py:194
msgid "Speaks track remaining time"
msgstr "مدت زمانِ باقی‌مانده از تِرَکِ در حالِ پخش را اعلام میکند."

#: addon/appModules/winamp.py:200 addon/appModules/winamp.py:220
#: addon/appModules/winamp.py:227
msgid "not playing"
msgstr "پخش نمی‌شود"

#: addon/appModules/winamp.py:201
msgid "Review the end of track (last 6 seconds by default)"
msgstr "بازشنویِ پایانِ تِرَک (واپسین ۶ ثانیه بطور پیش‌فرض)"

#: addon/appModules/winamp.py:207 addon/appModules/winamp.py:234
msgid "Seconds:"
msgstr "ثانیه"

#: addon/appModules/winamp.py:207
msgid "Set review time"
msgstr "تنظیمِ زمانِ بازشنوی"

#: addon/appModules/winamp.py:211 addon/appModules/winamp.py:238
msgid "Bad value entered! Please try again."
msgstr "مقدارِ وارد‌شده درست نیست! لطفا دوباره تلاش کنید."

#: addon/appModules/winamp.py:211 addon/appModules/winamp.py:238
msgid "Error"
msgstr "خطا"

#: addon/appModules/winamp.py:214
msgid ""
"Set the review time (in seconds) for use with Review End of Track command"
msgstr ""
"زمانِ بازشنوی را (به ثانیه) برای استفاده در فرمانِ بازشنویِ پایانِ تِرَک تنزیم "
"میکند."

#: addon/appModules/winamp.py:221
msgid "Alternate jump forward (6 seconds by default)"
msgstr "جلو رفتنِ جایگزین روی تِرَک (۶ ثانیه بطور پیش‌فرض)"

#: addon/appModules/winamp.py:228
msgid "Alternate jump backward (6 seconds by default)"
msgstr "عقب رفتنِ جایگزین روی تِرَک (۶ ثانیه بطور پیش‌فرض)"

#: addon/appModules/winamp.py:234
msgid "Set alternate jump time"
msgstr "تنظیمِ زمانِ عقب یا جلو رفتنِ جایگزین"

#: addon/appModules/winamp.py:242
msgid "Set alternate jump time (in seconds)"
msgstr "تنظیمِ زمانِ عقب یا جلو رفتنِ جایگزین  روی تِرَک (به ثانیه)"

