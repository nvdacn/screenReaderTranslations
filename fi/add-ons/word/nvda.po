#. Translators: A type of revision
msgid "No revision"
msgstr "Ei muutosta"

#. Translators: A type of revision
msgid "Insertion"
msgstr "Lisäys"

#. Translators: A type of revision
msgid "Deletion"
msgstr "Poisto"

#. Translators: Description for a revision
msgid "{revisionType} by {author}"
msgstr "Muokkaajan {author} {revisionType}"

