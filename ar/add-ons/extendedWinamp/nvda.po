#, python-format
msgid "%d hour, "
msgstr "%d ساعة"

#, python-format
msgid "%d hours, "
msgstr "%d ساعات, "

#, python-format
msgid "%d minute, "
msgstr "%d دقيقة, "

#, python-format
msgid "%d minutes, "
msgstr "%d دقائق"

#, python-format
msgid "%s second"
msgstr "%s ثانية"

#, python-format
msgid "%s seconds"
msgstr "%s ثوان"

msgid "on"
msgstr "تشغيل"

msgid "off"
msgstr "تعطيل"

msgid "mute"
msgstr "كتم"

msgid "Mute playback"
msgstr "كتم صوت الملف الحالي"

msgid "Set playback volume to 25%"
msgstr "ضبط مستوى صوت الملف حتى 25%"

msgid "Set playback volume to 50%"
msgstr "ضبط مستوى صوت الملف حتى 50%"

msgid "Set playback volume to 100%"
msgstr "ضبط مستوى صوت الملف حتى 100%"

msgid "Pan left"
msgstr "توجيه الصوت للسماعة اليسرى "

msgid "Pan right"
msgstr "توجيه الصوت للسماعة اليمنى"

msgid "center"
msgstr "تمركز الصوت بين السماعتين"

msgid "Pan center"
msgstr "تمركز الصوت بين السماعتين"

msgid "No time information."
msgstr "المعلومات عن الوقت غير متوافرة"

msgid "Speaks total track length"
msgstr "إعلان إجمالي مدة الملف"

msgid "Speaks track elapsed time"
msgstr "إعلان الوقت المنقضي"

msgid "Speaks track remaining time"
msgstr "إعلان الوقت المتبقي"

msgid "not playing"
msgstr "الملف لا يعمل"

msgid "Review the end of track (last 6 seconds by default)"
msgstr "استعراض نهاية الملف (6 ثواني بشكل افتراضي)"

msgid "Seconds:"
msgstr "ثواني"

msgid "Set review time"
msgstr "ضبط طريقة استعراض الوقت"

msgid "Bad value entered! Please try again."
msgstr "تم إدخال قيمة خاطئة، من فضلك حاول مرة أخرى"

msgid "Error"
msgstr "خطأ"

msgid ""
"Set the review time (in seconds) for use with Review End of Track command"
msgstr ""
"ضبط طريقة استعراض الوقت (بالثواني) كي تستخدم مع أمر استعراض نهاية الملف"

msgid "Alternate jump forward (6 seconds by default)"
msgstr "البديل الآخر للقفز للأمام (6 ثوانٍ بشكل افتراضي)"

msgid "Alternate jump backward (6 seconds by default)"
msgstr "البديل الآخر للقفز للخلف (6 ثوانٍ بشكل افتراضي)"

msgid "Set alternate jump time"
msgstr "ضبط طريقة القفز البديلة للوقت بالملف"

msgid "Set alternate jump time (in seconds)"
msgstr "ضبط بديل آخر للقفز بالوقت (بالثواني)"

