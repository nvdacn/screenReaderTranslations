msgstr "العربية"

msgid "Bangla"
msgstr "البنغالية"

msgid "Welsh"
msgstr "ويلزي"

msgid "Esperanto"
msgstr "الاسبرانتو لغة دولية"

msgid "Gujarati"
msgstr "الغوجاراتية"

msgid "Creole Haiti"
msgstr "الكريول هايتي"

msgid "Armenian"
msgstr "الأرمينية"

msgid "Latin"
msgstr "اللاتينية"

msgid "Norwegian"
msgstr "النرويجية"

msgid "Serbian (Latin)"
msgstr "السربية (الاتينية)"

msgid "Swahili"
msgstr "السواحلية"

msgid "Tagalog"
msgstr "التاغالوغية"

msgid "Yiddish"
msgstr "ايديش"

